	[COMMON_DOMESTIC]
	[PACK_ANIMAL]
	[WAGON_PULLER]
	[TRADE_CAPACITY:15000]
	[PET]}
{@SET;OBJECT_CURSOR;CREATURE:HONEY_BEE}
{@SET_VALUE;BODY:INSECT;2EYES:HEART:GUTS:BRAIN:MOUTH:2WINGS:SHELL;
	[UBIQUITOUS][VERMIN_FISH][FREQUENCY:100][PEARL]
	[AMPHIBIOUS][SMALL_REMAINS][FISHITEM][NOBONES][UNDERSWIM][COOKABLE_LIVE]}
{@FOREACH;CREATURE=0|TRANSLATION=0;
