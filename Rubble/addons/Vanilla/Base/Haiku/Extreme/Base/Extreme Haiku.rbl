{SHARED_OBJECT_ADD;PLANT:MUSHROOM_HELMET_PLUMP;{@TREE_GIVE_FRUIT;DARK_INDIGO;5:0:0;plump helmet;spawn;wine;true;true;true;true;true;true;false}$ABOVE_GROUND_BIOMES}
{SHARED_OBJECT_ADD;PLANT:MEADOW-GRASS;$SUBTERRANEAN_BIOMES}
